{
  "name": "vue2",
  "version": "1.0.0",
  "description": "vue2-demo",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js --progress",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack.prod.js --progress",
    "build:w": "npm run clean && cross-env NODE_ENV=production webpack --watch --config ./build/webpack.prod.js --progress",
    "clean": "rimraf ./dist/*",
    "open": "opn http://localhost:9090 && node ./server/server-dist.js",
    "prod": "npm run build && npm run open",
    "serve": "cd ./server && nodemon server-api.js"
  },
  "keywords": [
    "vue2",
    "vue-router",
    "webapck3"
  ],
